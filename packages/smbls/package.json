    "dist"
  ],
  "dependencies": {
    "@symbo.ls/cli": "^2.11.461",
    "@symbo.ls/create": "^2.11.461",
    "@symbo.ls/default-config": "^2.11.453",
    "@symbo.ls/init": "^2.11.453",
    "@symbo.ls/scratch": "^2.11.453",
    "@symbo.ls/sync": "^2.11.460",
    "@symbo.ls/uikit": "^2.11.455",
