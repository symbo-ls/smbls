    "domql": "^2.5.155"
  },
  "type": "module",
  "exports": "./dist/cjs/index.js",
  "publishConfig": {},
  "scripts": {
    "copy:package:cjs": "cp ../../build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild ./index.js --target=es2017 --format=esm --outdir=dist/esm --loader:.svg=text",
    "build:cjs": "npx esbuild ./index.js --target=node16 --format=cjs --loader:.svg=text",
    "build:cjs:bundle": "yarn build:cjs --bundle",
    "build": "yarn build:cjs --outdir=dist/cjs; yarn build:cjs:bundle --outfile=dist/cjs/index.bundle.js",
    "prepublish": "rimraf -I dist; yarn build; yarn copy:package:cjs"
  }
