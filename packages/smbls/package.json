{
  "name": "smbls",
  "version": "2.32.24",
  "license": "MIT",
  "repository": "https://github.com/symbo-ls/smbls",
  "gitHead": "7e05aa989bd6e42d442620e50981a93f35b898ce",
  "type": "module",
  "module": "index.js",
  "main": "index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "source": "index.js",
  "files": [
    "*.js",
    "dist"
  ],
  "dependencies": {
    "@symbo.ls/cli": "^2.32.24",
    "@symbo.ls/create": "^2.32.24",
    "@symbo.ls/default-config": "^2.32.24",
    "@symbo.ls/init": "^2.32.24",
    "@symbo.ls/scratch": "^2.32.24",
    "@symbo.ls/sync": "^2.32.24",
    "@symbo.ls/uikit": "^2.32.24",
    "@symbo.ls/utils": "^2.32.24",
    "attrs-in-props": "^2.32.24",
    "css-in-props": "^2.32.24",
    "domql": "^2.32.24"
  },
  "publishConfig": {},
  "scripts": {
    "copy:package:cjs": "cp ../../build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx cross-env NODE_ENV=$NODE_ENV npx esbuild ./index.js --target=es2017 --format=esm --outdir=dist/esm --loader:.svg=text",
    "build:cjs": "npx cross-env NODE_ENV=$NODE_ENV npx esbuild ./index.js --target=node16 --format=cjs --outdir=dist/cjs --loader:.svg=text",
    "build:cjs:bundle": "npm run build:cjs --bundle",
    "build": "npm run build:cjs; npm run build:cjs:bundle --outfile=dist/cjs/index.bundle.js; npm run build:esm;",
    "prepublish": "rimraf -I dist; npm run build; npm run copy:package:cjs"
  }
}
