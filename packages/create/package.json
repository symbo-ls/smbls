{
  "name": "@symbo.ls/create",
  "version": "2.25.1",
  "license": "MIT",
  "gitHead": "f4de780d7f974a235640526d303a3f3dc68ac1b4",
  "type": "module",
  "module": "src/index.js",
  "main": "src/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "exports": {
    ".": {
      "default": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "source": "src/index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "copy:package:cjs": "cp ../../build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild src/*.js --target=es2017 --format=esm --outdir=dist/esm",
    "build:cjs": "npx esbuild src/*.js --target=node16 --format=cjs --outdir=dist/cjs",
    "build:iife": "npx esbuild src/*.js --target=node16 --format=iife --outdir=dist/iife",
    "build": "rimraf -I dist; npm run build:cjs; npm run build:esm",
    "prepublish": "npm run build; npm run copy:package:cjs"
  },
  "dependencies": {
    "@domql/emotion": "^2.25.1",
    "@domql/event": "^2.25.1",
    "@domql/report": "^2.25.1",
    "@domql/router": "^2.25.1",
    "@symbo.ls/fetch": "^2.25.1",
    "@symbo.ls/init": "^2.25.1",
    "@symbo.ls/scratch": "^2.25.1",
    "@symbo.ls/sync": "^2.25.1",
    "@symbo.ls/uikit": "^2.25.1",
    "@symbo.ls/utils": "^2.25.1",
    "domql": "^2.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0"
  }
}
