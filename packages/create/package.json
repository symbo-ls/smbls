{
  "name": "@symbo.ls/create",
  "version": "3.0.5",
  "license": "MIT",
  "gitHead": "7744e475b747954ee8f0a8968651ff0eae11127a",
  "type": "module",
  "module": "src/index.js",
  "main": "src/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "exports": {
    ".": {
      "default": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "source": "src/index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "copy:package:cjs": "cp ../../build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild src/*.js --target=es2017 --format=esm --outdir=dist/esm",
    "build:cjs": "npx esbuild src/*.js --target=node16 --format=cjs --outdir=dist/cjs",
    "build:iife": "npx esbuild src/*.js --target=node16 --format=iife --outdir=dist/iife",
    "build": "rimraf -I dist; npm run build:cjs; npm run build:esm",
    "prepublish": "npm run build; npm run copy:package:cjs"
  },
  "dependencies": {
    "@domql/emotion": "^3.0.0",
    "@domql/event": "^3.0.0",
    "@domql/report": "^3.0.0",
    "@domql/router": "^3.0.0",
    "@symbo.ls/fetch": "^3.0.2",
    "@symbo.ls/init": "^3.0.2",
    "@symbo.ls/scratch": "^3.0.2",
    "@symbo.ls/sync": "^3.0.5",
    "@symbo.ls/uikit": "^3.0.5",
    "@symbo.ls/utils": "^3.0.2",
    "domql": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0"
  }
}
