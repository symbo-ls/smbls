{
  "name": "@symbo.ls/create",
  "version": "2.11.39",
  "license": "MIT",
  "gitHead": "bb04754011c9d569b2486b9add6f8e45a33001e4",
  "files": [
    "src",
    "dist"
  ],
  "repository": "https://github.com/symbo-ls/scratch",
  "type": "module",
  "module": "src/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "main": "src/index.js",
  "exports": "./dist/cjs/index.js",
  "source": "src/index.js",
  "publishConfig": {},
  "scripts": {
    "copy:package:cjs": "cp ../../build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild ./src/*.js --target=es2020 --format=esm --outdir=dist/esm",
    "build:cjs": "npx esbuild ./src/*.js --target=node16 --format=cjs --outdir=dist/cjs",
    "build:cjs:bundle": "npx esbuild ./src/*.js --target=node16 --format=cjs --outdir=dist/cjs/bundle --bundle",
    "build:iife": "npx esbuild ./src/index.js --target=es2020 --format=iife --outdir=dist/iife --bundle --minify",
    "build": "yarn build:cjs; yarn build:cjs:bundle",
    "prepublish": "rimraf -I dist && yarn build && yarn copy:package:cjs"
  },
  "dependencies": {
    "@domql/cookie": "latest",
    "@domql/emotion": "latest",
    "@domql/env": "latest",
    "@domql/report": "latest",
    "@domql/router": "latest",
    "@domql/utils": "latest",
    "@symbo.ls/fetch": "latest",
    "@symbo.ls/init": "latest",
    "@symbo.ls/scratch": "latest",
    "@symbo.ls/socket-ui": "latest",
    "@symbo.ls/uikit": "latest",
    "@symbo.ls/utils": "latest",
    "domql": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0"
  }
}
