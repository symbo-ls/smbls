  "author": "symbo.ls",
  "version": "2.10.149",
  "files": [
    "src",
    "dist"
  ],
  "repository": "https://github.com/symbo-ls/scratch",
  "type": "module",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "main": "dist/esm/index.js",
  "exports": "./dist/cjs/index.js",
  "source": "index.js",
  "publishConfig": {},
  "scripts": {
    "copy:package:cjs": "cp ../../.build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild *.js --target=es2020 --format=esm --outdir=dist/esm",
    "build:cjs": "npx esbuild *.js --target=node16 --format=cjs --outdir=dist/cjs",
    "build:iife": "npx esbuild *.js --target=node16 --format=iife --outdir=dist/iife",
    "build": "yarn build:esm && yarn build:cjs && yarn build:iife",
    "postinstall": "yarn build",
    "prepublish": "rimraf -I dist && yarn build && yarn copy:package:cjs"
  },
  "dependencies": {
    "@domql/utils": "latest",
    "@symbo.ls/utils": "latest",
    "color-contrast-checker": "^1.5.0"
  },
  "peerDependencies": {
    "@emotion/css": "^11.5.0"
  },
  "browserslist": [
    "> 1%",
    "ie >= 9"
  ],
