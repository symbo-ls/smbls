{
  "name": "smbls",
  "description": "UI Library built on Scratch and DOMQL",
  "private": false,
  "author": "symbo.ls",
  "version": "0.9.13",
  "repository": "https://github.com/symbo-ls/smbls",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "start": "parcel showcase/index.html",
    "build": "parcel build showcase/index.html --public-url .",
    "deploy": "yarn build && gh-pages -d dist",
    "clean": "rm yarn.lock && rm -rf node_modules && rm -rf dist && rm -rf .cache",
    "reinstall": "rm yarn.lock && rm -rf node_modules/rackai && yarn",
    "bump": "npx np"
  },
  "dependencies": {
    "@domql/utils": "latest",
    "@symbo.ls/init": "latest",
    "@symbo.ls/scratch": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "babel-eslint": "^10.1.0",
    "standard": "^13.1.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
