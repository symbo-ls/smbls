{
  "name": "@symbo.ls/init",
  "version": "1.0.6",
  "license": "MIT",
  "gitHead": "54fa6500c697604b3f48ba33a573545d7bff35fa",
  "dependencies": {
    "@domql/utils": "latest",
    "@emotion/css": "latest",
    "@symbo.ls/config-default": "latest",
    "@symbo.ls/scratch": "latest",
    "css-in-props": "latest"
  },
  "source": "src/index.js",
  "main": "dist/main.js",
  "targets": {
    "main": {
      "context": "browser",
      "outputFormat": "commonjs",
      "includeNodeModules": true
    }
  },
  "files": [ "dist", "README.md" ],
  "scripts": {
    "prebuild": "rm -rf dist .parcel-cache",
    "build": "parcel build index.js",
    "build:watch": "nodemon --exec \"yarn build\" --ignore 'dist/*' --watch ..",
    "test:lint": "npx standard \"packages/**/*.js\"",
    "test": "echo running all tests is not supported, maybe run: \"yarn test:lint\"",
    "push": "echo push is not yet supported",
    "push:version": "npm version patch && npm publish && git push origin --tags && git push",
    "clean": "rm -rf dist node_modules yarn.lock .parcel-cache yarn-error.log"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "babel-eslint": "^10.1.0",
    "emotion": "^10.0.27",
    "lerna": "^4.0.0",
    "parcel": "^2.0.1",
    "parcel-plugin-svg-sprite": "^1.4.1",
    "standard": "^13.1.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
