{
  "name": "smbls-bundle",
  "version": "2.10.188",
  "license": "MIT",
  "repository": "https://github.com/symbo-ls/smbls",
  "type": "module",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "main": "dist/esm/index.js",
  "exports": "./dist/cjs/index.js",
  "source": "index.js",
  "scripts": {
    "copy:package:cjs": "cp ../../../build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild ./index.js --target=es2020 --format=esm --outdir=dist/esm --bundle --minify",
    "build:cjs": "npx esbuild ./index.js --target=node16 --format=cjs --outdir=dist/cjs --bundle --minify",
    "build:iife": "npx esbuild ./index.js --target=es2020 --format=iife --outdir=dist/iife --bundle --minify",
    "build": "yarn build:esm && yarn build:cjs && yarn build:iife",
    "prepublish": "rimraf -I dist && yarn build && yarn copy:package:cjs"
  },
  "dependencies": {
    "smbls": "latest"
  },
  "files": [
    "index.js",
    "dist"
  ]
}
