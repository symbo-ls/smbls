{
  "name": "@symbo.ls/react-tooltip",
  "version": "2.11.195",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@symbo.ls/react-atoms": "latest",
    "@symbo.ls/react-icon": "latest",
    "@symbo.ls/tooltip": "latest"
  },
  "type": "module",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "main": "dist/esm/index.js",
  "exports": "./dist/cjs/index.js",
  "source": "index.js",
  "files": [
    "*.js",
    "dist"
  ],
  "scripts": {
    "copy:package:cjs": "cp ../../../build/package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild *.js --target=es2020 --format=esm --outdir=dist/esm --loader:.js=jsx",
    "build:cjs": "npx esbuild *.js --target=node16 --format=cjs --outdir=dist/cjs --loader:.js=jsx",
    "build:iife": "npx esbuild *.js --target=node16 --format=iife --outdir=dist/iife",
    "build": "yarn build:esm && yarn build:cjs",
    "prepublish": "rimraf -I dist && yarn build && yarn copy:package:cjs"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "gitHead": "75e0962f6d8d5c55467ed968e9aaabd97a729dfa"
}
