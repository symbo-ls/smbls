{
  "name": "@symbo.ls/react-atoms",
  "version": "2.10.139",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@symbo.ls/react-box": "latest",
    "css-in-props": "latest"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "type": "module",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "main": "dist/esm/index.js",
  "exports": "./dist/cjs/index.js",
  "source": "index.js",
  "files": [
    "*.jsx",
    "dist"
  ],
  "scripts": {
    "copy:package:cjs": "cp ../../package-cjs.json dist/cjs/package.json",
    "build:esm": "npx esbuild *.jsx --target=es2020 --format=esm --outdir=dist/esm",
    "build:cjs": "npx esbuild *.jsx --target=node16 --format=cjs --outdir=dist/cjs --loader:.js=jsx && yarn copy:package:cjs",
    "build:iife": "npx esbuild *.jsx --target=node16 --format=iife --outdir=dist/iife",
    "prepublish": "rimraf -I dist && yarn build:esm && yarn build:cjs && yarn build:iife"
  },
  "gitHead": "31dd387c4ad3fc2c3c49fb9e25f95469d5082464"
}
